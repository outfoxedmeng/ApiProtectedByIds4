{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ApiConfiguration": {
    "IdentityServerBaseUrl": "https://localhost:44310", //(必需)鉴权服务器
    "OidcApiName": "my_api", //(必需)鉴权服务器中的客户端/资源->API资源->名称
    "RequireHttpsMetadata": false, // 是否必需HTTPS
    "ApiScopesForClientCredentials": [
      "my_api_scope"
    ],
    "ApiName": "my_api",
    "OidcSwaggerUIClientId": "my_api_swaggerui"
    //"CorsAllowAnyOrigin": true,
    //"CorsAllowOrigins": [],
    //"AdministrationRole": "Administrator",
    //"ApiVersion": "v1",
    //"ApiBaseUrl": "https://localhost:5000",
  }
}
